      />
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/nume2"
        android:layout_marginBottom="20dp"
        android:text="Nume pacient:"
        />
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/sugestii"
        android:layout_marginBottom="20dp"
        android:text="Sugestii:"
        />
</LinearLayout>